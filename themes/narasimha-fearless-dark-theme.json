{
  "name": "Narasimha - Fearless Dark Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#0A0A0A",
    "editor.foreground": "#F8F8F2",
    "editorCursor.foreground": "#FF5F9E",
    "editor.lineHighlightBackground": "#151515",
    "editorLineNumber.foreground": "#3A3A3A",
    "editorLineNumber.activeForeground": "#FF5F9E",
    "editor.selectionBackground": "#264F78",
    "editor.inactiveSelectionBackground": "#2C2C3C88",
    "editorIndentGuide.background": "#222",
    "editorIndentGuide.activeBackground": "#FF00FF",
    "editorWhitespace.foreground": "#383838",
    "editorBracketMatch.background": "#222",
    "editorBracketMatch.border": "#FF5F9E",
    "editorGroupHeader.tabsBackground": "#000000",
    "activityBar.background": "#0D0D0D",
    "activityBar.foreground": "#FFFFA5",
    "activityBar.activeBorder": "#FFFFA5",

    "sideBar.background": "#1B1D2B",
    "sideBar.foreground": "#C0CAF5",
    "sideBarSectionHeader.background": "#1F2233",
    "sideBarSectionHeader.foreground": "#82AAFF",
    "list.hoverBackground": "#2C2F41",
    "list.activeSelectionBackground": "#3E445E",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#2B2D3A",
    "list.inactiveSelectionForeground": "#C0CAF5",
    "tree.indentGuidesStroke": "#2F3244",

    "statusBar.background": "#0D0D0D",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.debuggingBackground": "#5F00FF",

    "tab.activeBackground": "#5F00FF",
    "tab.inactiveBackground": "#0D0D0D",
    "tab.activeForeground": "#FFFFA5",
    "tab.inactiveForeground": "#888888",
    "tab.border": "#1E1E2F",

    "titleBar.activeBackground": "#5F00FF",
    "titleBar.activeForeground": "#FFFFA5",
    "titleBar.inactiveBackground": "#101010",
    "titleBar.inactiveForeground": "#888888",

    "terminal.background": "#0D0D0D",
    "terminal.foreground": "#F8F8F2",
    "terminalCursor.foreground": "#FF5F9E",
    "terminal.ansiBlack": "#21222C",
    "terminal.ansiRed": "#FF5555",
    "terminal.ansiGreen": "#50FA7B",
    "terminal.ansiYellow": "#F1FA8C",
    "terminal.ansiBlue": "#8BE9FD",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#F8F8F2",
    "terminal.ansiBrightBlack": "#6272A4",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#69FF94",
    "terminal.ansiBrightYellow": "#FFFFA5",
    "terminal.ansiBrightBlue": "#D6ACFF",
    "terminal.ansiBrightMagenta": "#FF92DF",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF5F9E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#FFFD82"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#9AEDFE"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#00FFC6"
      }
    },
    {
      "scope": ["variable.parameter", "meta.function.parameters"],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": ["entity.name.type", "support.class", "meta.class", "storage.type.class"],
      "settings": {
        "foreground": "#A799FF"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#FF73FA"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter", "punctuation.definition"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#FF0000"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9A8B"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#00BFFF"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.readonly.defaultLibrary": "#E2E2E2",
    "parameter": "#FFB86C",
    "property": "#00FFF7",
    "function": "#00FFAA",
    "class": "#A799FF",
    "type": "#FF80FF",
    "enum": "#FFDA44",
    "interface": "#FF73FA",
    "variable.defaultLibrary": "#A0A0A0"
  }
}
