{
    "name": "Bhoot - Cool Light Theme",
    "type": "light",
    "colors": {
      "editor.background": "#ffffff",
      "editor.foreground": "#000000",
      "editorLineNumber.foreground": "#A0A1A7",
      "editorCursor.foreground": "#D75F5F",
      "editor.selectionBackground": "#D0D0D0",
      "editor.inactiveSelectionBackground": "#EDEDED",
      "editor.lineHighlightBackground": "#EFEFEF",
      "editorIndentGuide.background": "#E0E0E0",
      "editorIndentGuide.activeBackground": "#C0C0C0",
      "editorGroupHeader.tabsBackground": "#faeb69",
      "tab.border": "#faeb69",
      "editorGroup.border": "#faeb69",
      "sideBar.background": "#ffffe6",
      "sideBar.foreground": "#000000",
      "activityBar.background": "#faeb69",
      "activityBar.foreground": "#331a00",
      "tab.activeBackground": "#FFFFFF",
      "tab.activeForeground": "#005F87",
      "tab.selectedBorderTop": "#331a00",
      "tab.inactiveBackground": "#faeb69",
      "tab.inactiveForeground": "#000000",
      "statusBar.background": "#ffffe6",
      "statusBar.foreground": "#000000",
      "statusBar.noFolderBackground": "#ffffe6",
      "statusBarItem.remoteBackground": "#331a00",
      "titleBar.activeBackground": "#faeb69",
      "titleBar.activeForeground": "#000000",
      "terminal.background": "#e6fff2",
      "terminal.foreground": "#383A42",
      "button.background": "#e6fff2",
      "button.foreground": "#000000",
      "button.border": "#331a00",
      "badge.background": "#331a00",
      "badge.foreground": "#ffffff"
    },
    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#A0A1A7",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword", "storage.type"],
        "settings": {
          "foreground": "#4d4dff",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["string", "constant.other.symbol"],
        "settings": {
          "foreground": "#50A14F"
        }
      },
      {
        "scope": ["variable", "identifier"],
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "scope": ["function", "entity.name.function"],
        "settings": {
          "foreground": "#4078F2"
          
        }
      },
      {
        "scope": ["constant.numeric", "constant.language"],
        "settings": {
          "foreground": "#986801"
        }
      },
      {
        "scope": ["type", "entity.name.type", "support.type"],
        "settings": {
          "foreground": "#6b00b3",
          "fontStyle": "underline"
        }
      },
      {
        "scope": ["invalid", "invalid.deprecated"],
        "settings": {
          "foreground": "#FFFFFF",
          "background": "#E45649"
        }
      },
      {
        "scope": ["meta", "meta.embedded"],
        "settings": {
          "foreground": "#0184BC"
        }
      }
    ]
  }
  