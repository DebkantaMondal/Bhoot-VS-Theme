    "tab.hoverBackground": "#D1D1D1",
    "tab.border": "#E1E1E1",
    "tab.unfocusedActiveBackground": "#F5F5F5",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#1B1D2B",
    "terminalCursor.background": "#FFD700",
