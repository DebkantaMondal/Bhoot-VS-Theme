{
    "name": "Bhoot - Cool Light Theme",
    "type": "light",
    "colors": {
      "editor.background": "#FAFAFA",
      "editor.foreground": "#383A42",
      "editorLineNumber.foreground": "#A0A1A7",
      "editorCursor.foreground": "#D75F5F",
      "editor.selectionBackground": "#D0D0D0",
      "editor.inactiveSelectionBackground": "#EDEDED",
      "editor.lineHighlightBackground": "#EFEFEF",
      "editorIndentGuide.background": "#E0E0E0",
      "editorIndentGuide.activeBackground": "#C0C0C0",
      "sideBar.background": "#F5F5F5",
      "sideBar.foreground": "#4B4B4B",
      "activityBar.background": "#EAEAEA",
      "activityBar.foreground": "#005F87",
      "tab.activeBackground": "#FFFFFF",
      "tab.activeForeground": "#005F87",
      "tab.inactiveBackground": "#F0F0F0",
      "tab.inactiveForeground": "#7A7A7A",
      "statusBar.background": "#E0E0E0",
      "statusBar.foreground": "#383A42",
      "titleBar.activeBackground": "#FAFAFA",
      "titleBar.activeForeground": "#383A42",
      "terminal.background": "#FAFAFA",
      "terminal.foreground": "#383A42"
    },
    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#A0A1A7",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword", "storage.type"],
        "settings": {
          "foreground": "#D75F5F",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["string", "constant.other.symbol"],
        "settings": {
          "foreground": "#50A14F"
        }
      },
      {
        "scope": ["variable", "identifier"],
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "scope": ["function", "entity.name.function"],
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "scope": ["constant.numeric", "constant.language"],
        "settings": {
          "foreground": "#986801"
        }
      },
      {
        "scope": ["type", "entity.name.type", "support.type"],
        "settings": {
          "foreground": "#C18401"
        }
      },
      {
        "scope": ["invalid", "invalid.deprecated"],
        "settings": {
          "foreground": "#FFFFFF",
          "background": "#E45649"
        }
      },
      {
        "scope": ["meta", "meta.embedded"],
        "settings": {
          "foreground": "#0184BC"
        }
      }
    ]
  }
  