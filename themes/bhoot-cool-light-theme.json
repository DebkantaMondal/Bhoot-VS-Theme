    "terminal.ansiWhite": "#C0CAF5",
    "terminal.ansiBrightBlack": "#D1D1D1",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#B9F27C",
    "terminal.ansiBrightYellow": "#FFD700",
    "terminal.ansiBrightBlue": "#8BE9FD",
    "terminal.ansiBrightMagenta": "#D1D1D1",
