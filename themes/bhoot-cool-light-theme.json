    "titleBar.inactiveForeground": "#8BE9FD",
    "titleBar.border": "#E1E1E1"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#A0A1A7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#4d4dff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#50A14F"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#383A42"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#4078F2"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#986801"
      }
    },
    {
      "scope": [
        "type",
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#6b00b3",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#E45649"
      }
    },
    {
      "scope": [
        "meta",
        "meta.embedded"
      ],
      "settings": {
        "foreground": "#0184BC"
      }
    }
  ]
}