{
    "name": "Bhoot - Default Dark Theme",
    "type": "dark",
    "colors": {
      "editor.background": "#1E1E2E",
      "editor.foreground": "#D9E0EE",
      "editorLineNumber.foreground": "#FFFFFF",
      "editorCursor.foreground": "#faeb69",
      "editor.selectionBackground": "#000000",
      "editor.inactiveSelectionBackground": "#343A40",
      "editor.lineHighlightBackground": "#2A2A37",
      "editorIndentGuide.background": "#000000",
      "editorIndentGuide.activeBackground": "#CBA6F7",
      "sideBar.background": "#22222e",
      "sideBar.foreground": "#A6ADC8",
      "activityBar.background": "#000000",
      "activityBar.foreground": "#ccf2ff",
      "tab.activeBackground": "#1E1E2E",
      "tab.activeForeground": "#FFFFFF",
      "tab.selectedBorderTop": "#ccf2ff",
      "tab.border": "#000000",
      "tab.inactiveBackground": "#000000",
      "tab.inactiveForeground": "#6E738D",
      "editorGroupHeader.tabsBackground": "#000000",
      "editorGroup.border": "#000000",
      "statusBar.background": "#66ff66",
      "statusBar.noFolderBackground": "#66ff66",
      "statusBarItem.remoteBackground": "#004d1a",
      "statusBar.foreground": "#000000",
      "titleBar.activeBackground": "#000000",
      "titleBar.activeForeground": "#D9E0EE",
      "terminal.background": "#e6ffee",
      "terminal.foreground": "#1E1E2E",
      "terminal.fontStyle": "bold italic",
      "button.background": "#050517",
      "button.border": "#FFFFFF",
      "button.foreground": "#FFFFFF",
      "textLink.foreground": "#faeb69",
      "banner.background": "#FFFFFF"
    },
    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#6C7086",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword", "storage.type"],
        "settings": {
          "foreground": "#ffb3ff",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["string", "constant.other.symbol"],
        "settings": {
          "foreground": "#A6E3A1"
        }
      },
      {
        "scope": ["variable", "identifier"],
        "settings": {
          "foreground": "#faeb69"
        }
      },
      {
        "scope": ["function", "entity.name.function"],
        "settings": {
          "foreground": "#89B4FA"
        }
      },
      {
        "scope": ["constant.numeric", "constant.language"],
        "settings": {
          "foreground": "#F9E2AF"
        }
      },
      {
        "scope": ["type", "entity.name.type", "support.type"],
        "settings": {
          "foreground": "#e6fff7",
          "fontStyle": "underline"
        }
      },
      {
        "scope": ["invalid", "invalid.deprecated"],
        "settings": {
          "foreground": "#FFFFFF",
          "background": "#F38BA8"
        }
      },
      {
        "scope": ["meta", "meta.embedded"],
        "settings": {
          "foreground": "#94E2D5"
        }
      }
    ]
  }
  