    "notifications.errorForeground": "#1B1D2B",
    "notifications.successBackground": "#B9F27C88",
    "notifications.successForeground": "#1B1D2B"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6C7086",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ffb3ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#A6E3A1"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#89B4FA"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#F9E2AF"
      }
    },
    {
      "scope": [
        "type",
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#00fff799",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F38BA8"
      }
    },
    {
      "scope": [
        "meta",
        "meta.embedded"
      ],
      "settings": {
        "foreground": "#94E2D5"
      }
    },
    {
      "name": "HTML Tag Name",
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#ff99cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#7DDFFF"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "HTML Comment",
      "scope": "comment.block.html",
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Selector",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "CSS Property Name",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#7FDBCA"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": "constant.numeric.css, constant.other.color.rgb-value.css",
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Unit",
      "scope": "keyword.other.unit.css",
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "CSS Braces",
      "scope": [
        "punctuation.definition.block.css"
      ],
      "settings": {
        "foreground": "#A6ACCD"
      }
    },
    {
      "name": "JS Keywords",
      "scope": "keyword.control.js",
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JS Function Name",
      "scope": "entity.name.function.js",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "JS Variable",
      "scope": "variable.other.readwrite.js",
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "JS String",
      "scope": "string.quoted.double.js, string.quoted.single.js",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "JS Number & Boolean",
      "scope": "constant.numeric.js, constant.language.boolean.js",
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JS Brackets & Operators",
      "scope": "punctuation.definition.block.js, keyword.operator.js",
      "settings": {
        "foreground": "#89DDFF"
      }
    }
  ]
}